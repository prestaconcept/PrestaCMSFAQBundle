{
    "name": "presta/cms-faq-bundle",
    "type": "symfony-bundle",
    "description": "PrestaCMS FAQ bundle : Adds FAQ in PrestaCMS. FAQ are structured by category and administrated with SonataAdmin",
    "keywords": ["symfony2", "bundle", "cms", "symfony-cmf", "prestaconcept", "faq"],
    "homepage": "https://github.com/prestaconcept/PrestaCMSFAQBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Bastien",
            "email": "nbastien@prestaconcept.net",
            "homepage": "http://www.prestaconcept.net/"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.2",
        "presta/cms-core-bundle": "1.*",
        "presta/sonata-admin-extended-bundle": "1.*"
    },
    "autoload": {
        "psr-0": { "Presta\\CMSFAQBundle": "" }
    },
    "target-dir": "Presta/CMSFAQBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
